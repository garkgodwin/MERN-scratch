{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "lib": ["DOM", "ES2023", "ScriptHost"],
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "allowJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "useUnknownInCatchVariables": false
  },
  "include": ["src"]
}
